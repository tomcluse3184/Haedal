#include <stdio.h>

int main(void) {
    int num[9];
    int max, index;

    // 9개의 자연수 입력 받기
    for (int i = 0; i < 9; i++) {
        scanf("%d", &num[i]);
    }

    max = num[0];
    index = 1;

    // 최댓값과 그 위치 찾기
    for (int i = 1; i < 9; i++) {
        if (num[i] > max) {
            max = num[i];
            index = i + 1;  // 배열은 0부터 시작하므로 +1
        }
    }

    // 결과 출력
    printf("%d\n%d\n", max, index);

    return 0;
}

