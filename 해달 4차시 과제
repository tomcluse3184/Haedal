//최댓값
#include <stdio.h>

#define SIZE 9

typedef struct {
    int row; 
    int col;
    int num;  
} Result;

Result findMax(int arr[SIZE][SIZE]) {
    Result r;
    r.num = arr[0][0];
    r.row = 1;
    r.col = 1;

    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            if (arr[i][j] > r.num) {
                r.num = arr[i][j];
                r.row = i + 1; 
                r.col = j + 1;
            }
        }
    }

    return r;
}

int main(void) {
    int arr[SIZE][SIZE];
    Result ans;


    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            scanf("%d", &arr[i][j]);
        }
    }
    ans = findMax(arr);

    printf("%d\n%d %d\n", ans.num, ans.row, ans.col);

    return 0;
}

//네 번째 점 
#include <stdio.h>

int main(void) {
    int x1, y1, x2, y2, x3, y3;
    int x4, y4;

    scanf("%d %d", &x1, &y1);
    scanf("%d %d", &x2, &y2);
    scanf("%d %d", &x3, &y3);

    if (x1 == x2) x4 = x3;
    else if (x1 == x3) x4 = x2;
    else x4 = x1;

    if (y1 == y2) y4 = y3;
    else if (y1 == y3) y4 = y2;
    else y4 = y1;

    printf("%d %d\n", x4, y4);

    return 0;
}

//덩치
#include <stdio.h>

int main(void) {
    int N;
    int weight[50], height[50];
    int rank[50];

    scanf("%d", &N);

    for (int i = 0; i < N; i++) {
        scanf("%d %d", &weight[i], &height[i]);
        rank[i] = 1;
    }

    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            if (i == j) continue;
            if (weight[j] > weight[i] && height[j] > height[i]) {
                rank[i]++; 
            }
        }
    }

  
    for (int i = 0; i < N; i++) {
        printf("%d ", rank[i]);
    }
    printf("\n");

    return 0;
}

